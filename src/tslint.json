{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "prefer-const": false,
        "object-literal-shorthand": false,
        "only-arrow-functions": false,
        "triple-equals": false,
        "variable-name": false
    }
}
